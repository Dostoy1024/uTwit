<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<umbPackage>
	<info>
		<package>
			<name>uTwit Web</name>
			<version>0.0</version>
			<license url="http://www.opensource.org/licenses/mit-license.php">MIT license</license>
			<url>http://our.umbraco.org/projects/website-utilities/utwit</url>
			<requirements>
				<major>4</major>
				<minor>11</minor>
				<patch>0</patch>
			</requirements>
		</package>
		<author>
			<name>Matt Brailsford</name>
			<website>http://theoutfield.net</website>
		</author>
		<readme><![CDATA[uTwit Web contains the necessary code for displaying your latest tweets on a website.
		
		NB: uTwit Web must be used in combination with uTwit DataType]]></readme>
	</info>

	<DocumentTypes />
	<Templates />
	<Stylesheets />
	<DictionaryItems />
	<Languages />
	<DataTypes />
	<Macros>
		<macro>
			<name>Latest Tweets</name>
			<alias>latestTweets</alias>
			<scriptType></scriptType>
			<scriptAssembly></scriptAssembly>
			<xslt>latestTweets.xslt</xslt>
			<useInEditor>False</useInEditor>
			<refreshRate>60</refreshRate>
			<scriptingFile></scriptingFile>
			<properties />
		</macro> 
		<macro>
			<name>Search Tweets</name>
			<alias>searchTweets</alias>
			<scriptType></scriptType>
			<scriptAssembly></scriptAssembly>
			<xslt>searchTweets.xslt</xslt>
			<useInEditor>False</useInEditor>
			<refreshRate>60</refreshRate>
			<scriptingFile></scriptingFile>
			<properties />
		</macro>
		<macro>
			<name>Twitter Profile</name>
			<alias>twitterProfile</alias>
			<scriptType></scriptType>
			<scriptAssembly></scriptAssembly>
			<xslt>twitterProfile.xslt</xslt>
			<useInEditor>False</useInEditor>
			<refreshRate>60</refreshRate>
			<scriptingFile></scriptingFile>
			<properties />
		</macro>  
	</Macros>

	<Actions />

	<control />

	<files />

</umbPackage>